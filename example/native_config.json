{
  "adb_path": "/opt/platform-tools/adb",
  "monkeyrunner_path": "/opt/platform-tools/bin/monkeyrunner",
  "type": "native",
  "devices": ["5x"],
  "replications": 2,
  "paths": [
    "/home/yoyo/android-runner/example/coolcherrytrees.games.reactor4.apk"
  ],
  "profilers": {
    "trepn": {
      "sample_interval": 100,
      "data_points": [
        "battery_power", "mem_usage"
      ]
    },
    "volta": {}
 },
  "scripts": {
    "before_experiment": "before_experiment.py",
    "before_run": "before_run.py",
    "interaction": [
      {
        "type": "python",
        "path": "interaction.py",
        "timeout": 500,
        "logcat_regex": "some keyword"
      },
      {
        "type": "monkeyrunner",
        "path": "monkey.py"
      }
    ],
    "after_run": "after_run.py",
    "after_experiment": "after_experiment.py"
  },
  "time_between_run": 500
}