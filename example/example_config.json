{
  "name": "Some web experiment",
  "base_directory": "~/experiment/",
  "interface": "adb",
  "devices": ["g2", "5x", "6p"],
  "runs": 50,
  "type": "web",
  "paths": [
    "https://www.google.com/",
    "https://github.com/"
  ],
  "browsers": ["chrome", "opera", "firefox"],
  "metrics": {
    "energy": {
      "tool": "trepn",
      "sample_every": 100,
      "output": "./energy/"
    },
    "performance": {
      "tool": "adb",
      "sample_every": 100,
      "output": "./performance/"
    }
  },
  "scripts": {
    "device_setup": "device_setup.py",
    "experiment_setup": "experiment_setup.py",
    "experiment": "experiment.py",
    "experiment_teardown": "experiment_teardown.py",
    "device_teardown": "device_teardown.py"
  },
  "aggregation": ""
}